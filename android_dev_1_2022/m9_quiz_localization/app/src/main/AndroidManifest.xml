<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.m9_quiz_localization">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Simple_theme">
        <activity
            android:name="com.example.m9_quiz_localization.navigation.NavigationActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Simple_theme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.m9_quiz_localization.MainActivity"
            android:exported="true">
        </activity>
    </application>
</manifest>