<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kitchen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kitchen">
        <activity
            android:name=".module14.MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
        </activity>
        <activity
            android:name=".module14.DynamicActivity">
        </activity>

        <activity
            android:name=".module15.ToolbarActivity"
            android:label="@string/toolbar"
            android:parentActivityName=".module14.MainActivity">
        </activity>
        <activity
            android:name=".module15.CoordinatorActivity"
            android:label="@string/coordinator"
            android:parentActivityName=".module14.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module16.MainActivity"
            android:label="@string/parcelable"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SENDTO" />
                <data android:scheme="mailto"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".module17.SecondActivity"
            android:label="@string/intent"
            android:exported="true">
        </activity>
    </application>

</manifest>